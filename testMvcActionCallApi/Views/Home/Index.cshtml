@model List<testMvcActionCallApi.Models.Product>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <h2>Id: @item.id</h2>
                <h2>Name: @item.name </h2>
                <h2>Price: $NT. @item.price</h2>
            </div>
        }
    </div>
   
</div>
 